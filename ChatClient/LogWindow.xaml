<Window x:Class="ChatClient.LogWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ChatClient"
        mc:Ignorable="d"
        Title="Лог клиента" Height="400" Width="600" ResizeMode="NoResize">
    <Grid>
        <TextBox x:Name="tbLog" HorizontalAlignment="Center" Height="364" TextWrapping="Wrap" IsReadOnly="True" VerticalAlignment="Center" Width="580" BorderThickness="0" FontSize="12"/>
    </Grid>
</Window>